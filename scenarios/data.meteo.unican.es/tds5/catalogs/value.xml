				<dataSize units="Mbytes">25527</dataSize>
				<date type="modified">2014-12-19T03:46:09Z</date>
			</dataset>
			<datasetScan name="individual files" ID="interim20_daily_files"
				path="interim20_daily_files" location="//oceano/gmeteo/WORK/ensembles/downscalingPortal/GCMData/ERA_INTERIM_DM/data" restrictAccess="interim20_sd">
