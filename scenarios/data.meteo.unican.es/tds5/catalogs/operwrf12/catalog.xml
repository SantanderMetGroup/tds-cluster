   </service>

  <service name="multiService2" serviceType="Compound" base="" >
       <service name="thisDODS" serviceType="OpenDAP" base="/thredds/dodsC/" />
       <service name="thisHTTP" serviceType="HTTPServer" base="/thredds/fileServer/" />
       <service name="wcsServer" serviceType="WCS" base="/thredds/wcs/" />
       <service name="wmsServer" serviceType="WMS" base="/thredds/wms/" />
       <service name="ncServer" serviceType="NetcdfSubset" base="/thredds/ncss/grid/" />
	   <service name="mys" serviceType="queryCapability" base="/thredds/dqc/"/>  
   </service>
   
  <datasetRoot path="test" location="content/testdata/"/>

  <dataset name="Test Single Dataset" ID="testDataset" serviceName="thisDODS"
           urlPath="test/testData.nc"/>

  <datasetScan name="Test all files in a directory" ID="testDatasetScan"
               path="testAll" location="/oceano/gmeteo/DATA/CERA/">

    <metadata inherited="true">
      <serviceName>multiService</serviceName>
    </metadata>

    <filter>
      <include wildcard="*.nc"/>
      <include wildcard="*.ncml"/>
    </filter>

  </datasetScan>
  <datasetScan name="ALLCNRM" ID="CNRMDataSet"
               path="cnrm" location="/oceano/gmeteo/DATA/CNRM/CNRM-CM3/">

    <metadata inherited="true">
      <serviceName>multiService</serviceName>
    </metadata>

    <filter>
      <include wildcard="*.ncml"/>
    </filter>

  </datasetScan>
  
  <dataset name="WRF-UC" ID="OPERWRF12Dataset">
    <metadata inherited="true">
      <serviceName>multiService</serviceName>
      <authority>es.unican.meteo</authority>
      <dataType>Grid</dataType>
      <dataFormat>NetCDF</dataFormat>
      <documentation type="rights">The user assumes the entire risk related to its use of this data. Santander Meteo Group (SMG) is providing this data "as is," and SMG disclaims any and all warranties, whether express or implied, including (without limitation) any implied warranties of merchantability or fitness for a particular purpose. In no event will SMG be liable to you or to any third party for any direct, indirect, incidental, consequential, special or exemplary damages or lost profit resulting from any use or misuse of this data.</documentation>
      <documentation xlink:href="http://www.meteo.unican.es/projects/localForecast" xlink:title="Local Weather Forecast: Comparison of Statistical and Dynamical Methods" />

      
      <timeCoverage>
        <end>present</end>
        <duration>14 days</duration>
      </timeCoverage>

      <geospatialCoverage>
          <northsouth>
            <start>26.92475</start>
            <size>15.9778</size>
            <units>degrees_north</units>
          </northsouth>
          <eastwest>
            <start>-135.33123</start>
            <size>103.78772</size>
            <units>degrees_east</units>
          </eastwest>
        </geospatialCoverage>
        
       <variables vocabulary="CF-1">
          <variable name="Z_sfc" vocabulary_name="geopotential_height" units="gp m">Geopotential height, gpm</variable>
        </variables>
      </metadata>

  <datasetScan name="WRF-UC" ID="OPERWRF12DatasetScan"
               path="WRFUC" location="/oceano/services/DATASETS/OPERWRF12">

    <addProxies>
      <simpleLatest />
    </addProxies>

    <filter>
      <include wildcard="*.nc"/>
      <include wildcard="*.ncml"/>
      <include wildcard="????????12" atomic="false" collection="true"/>
    </filter>
    <addDatasetSize />
  </datasetScan>
 </dataset>
<featureCollection name="OPERWRFCollection" featureType="FMRC" path="fmrc/WRF-UC/GFS/oper_gfs_d01">
  <collection spec="/oceano/gmeteo/DATA/UC/OPERWRF12/**/oper_gfs#yyyyMMddHH#__4_1_1_1_1_1_1_d01.nc$"
          recheckAfter="15 min" olderThan="5 min"/>

</featureCollection>


